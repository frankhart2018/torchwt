{
    "model": {
        "name":"YOLOv3"
    },
    "loss": {
        "type": "binary_crossentropy"
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.00001,
            "weight_decay": 0.0001
        }
    },
    "training": {
        "args": {
            "epochs": 100,
            "map_iou_thresh": 0.5,
            "nms_iou_thresh": 0.45,
            "conf_threshold": 0.05
        }
    },
    "valid": {},
    "dataset": {
        "args": {
            "batch_size": 32,
            "image_size": 416,
            "shuffle": "True",
            "split_ratio": 0.8
        }
    }
}